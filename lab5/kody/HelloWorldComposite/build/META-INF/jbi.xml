<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<jbi xmlns="http://java.sun.com/xml/ns/jbi" xmlns:ns1="HelloWorldComposite" xmlns:ns2="http://enterprise.netbeans.org/bpel/HelloWorld/helloWorld" xmlns:ns3="http://j2ee.netbeans.org/wsdl/HelloWorld/src/HelloWorld" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="1.0" xsi:schemaLocation="http://java.sun.com/xml/ns/jbi ./jbi.xsd">
    <service-assembly>
        <identification>
            <name>HelloWorldComposite</name>
            <description>Represents the Service Assembly of HelloWorldComposite</description>
        </identification>
        <service-unit>
            <identification>
                <name>HelloWorldComposite-HelloWorld</name>
                <description>Represents this Service Unit</description>
            </identification>
            <target>
                <artifacts-zip>HelloWorld.jar</artifacts-zip>
                <component-name>sun-bpel-engine</component-name>
            </target>
        </service-unit>
        <service-unit>
            <identification>
                <name>HelloWorldComposite-sun-http-binding</name>
                <description>Represents this Service Unit</description>
            </identification>
            <target>
                <artifacts-zip>sun-http-binding.jar</artifacts-zip>
                <component-name>sun-http-binding</component-name>
            </target>
        </service-unit>
        <connections>
            <connection>
                <consumer endpoint-name="casaPort1" service-name="ns1:HelloWorldCompositeService1"/>
                <provider endpoint-name="HelloWorldPortTypeRole_myRole" service-name="ns2:PartnerLink1"/>
            </connection>
        </connections>
        <route-types xmlns="http://www.open-esb.net/xml/ns/jbi">
            <source endpoint-name="casaPort1" mode="local" routing-rule="default" service-name="ns1:HelloWorldCompositeService1">direct</source>
        </route-types>
        <interface-mapping xmlns="http://www.open-esb.net/xml/ns/jbi">
            <implementation>
                <by-provider endpoint-name="HelloWorldPortTypeRole_myRole" service-name="ns2:PartnerLink1"/>
                <interface name="ns3:HelloWorldPortType"/>
            </implementation>
        </interface-mapping>
    </service-assembly>
</jbi>
